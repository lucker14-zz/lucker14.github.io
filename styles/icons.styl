@charset "UTF-8"

@font-face
    font-family "twisto-icons"
    src: url("../fonts/twisto-icons.eot")
    src: url("../fonts/twisto-icons.eot?#iefix") format("embedded-opentype"),
        url("../fonts/twisto-icons.woff") format("woff"),
        url("../fonts/twisto-icons.ttf") format("truetype"),
        url("../fonts/twisto-icons.svg#twisto-icons") format("svg")
    font-weight normal
    font-style normal

[data-icon]:before
    font-family "twisto-icons" !important
    content attr(data-icon)
    font-style normal !important
    font-weight normal !important
    font-variant normal !important
    text-transform none !important
    speak none
    line-height 1
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

[class^="icon-"]:before,
[class*=" icon-"]:before
    font-family "twisto-icons" !important
    font-style normal !important
    font-weight normal !important
    font-variant normal !important
    text-transform none !important
    speak none
    line-height 1
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

